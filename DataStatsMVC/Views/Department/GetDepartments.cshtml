@model IEnumerable<Department>

<h2 class="mt-6">Departments</h2>

<table class="table table-hover mt-2">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Deparment</th>
            <th scope="col">Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var dep in Model)
        {
            <tr>
                <td scope="row">#</td>
                <td scope="row">@dep.Name</td>
                <td scope="row">@dep.DepartmentId</td>
            </tr>
        }
    </tbody>
</table>
