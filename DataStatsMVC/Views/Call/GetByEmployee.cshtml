@model IEnumerable<Call>

<table class="table-secondary">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Deparment</th>
            <th scope="col">Date</th>
            <th scope="col">AHT</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var call in Model)
        {
            <tr>
                <td scope="row">#</td>
                <td scope="row">@call.Department.Name</td>
                <td scope="row">@call.Start.ToShortDateString()</td>
                <td scope="row">@call.Employee.FirstName</td>
            </tr>
        }
    </tbody>
</table>